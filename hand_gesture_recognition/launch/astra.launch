<launch>
    
    <arg name="model_name" default="InceptionV3"/>

    <group ns="hgr">
        <rosparam file="$(find hand_gesture_recognition)/config/astra.yaml" />
    </group>

    <include file="$(find astra_camera)/launch/astra_pro.launch"/>

    <include file="$(find hand_gesture_recognition)/launch/hgr.launch">
        <arg name="model_name" value="$(arg model_name)"/>
    </include>

</launch>